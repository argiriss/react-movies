{
  "name": "react-movies",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.2.0",
    "@testing-library/user-event": "^13.5.0",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "plugins": [
      "react"
    ],
    "overrides": [
      {
        "files": [
          "**/*.js"
        ],
        "rules": {
          "no-unused-vars": "warn",
          "curly": [
            "error",
            "all"
          ],
          "brace-style": [
            "error",
            "1tbs",
            {
              "allowSingleLine": false
            }
          ],
          "comma-dangle": [
            "error",
            {
              "arrays": "always-multiline",
              "objects": "always-multiline",
              "imports": "always-multiline",
              "exports": "always-multiline",
              "functions": "never"
            }
          ],
          "no-console": "warn",
          "semi": [
            "error",
            "always"
          ],
          "jsx-quotes": [
            "error",
            "prefer-double"
          ],
          "max-len": [
            "error",
            {
              "code": 200
            }
          ],
          "padding-line-between-statements": [
            "error",
            {
              "blankLine": "always",
              "prev": "*",
              "next": "return"
            },
            {
              "blankLine": "always",
              "prev": [
                "const",
                "let",
                "var"
              ],
              "next": "*"
            },
            {
              "blankLine": "any",
              "prev": [
                "const",
                "let",
                "var"
              ],
              "next": [
                "const",
                "let",
                "var"
              ]
            }
          ],
          "object-curly-spacing": [
            "error",
            "always"
          ],
          "sort-imports": "off",
          "import/order": [
            "error",
            {
              "groups": [
                "builtin",
                "external",
                "internal",
                [
                  "parent",
                  "sibling"
                ]
              ],
              "alphabetize": {
                "order": "asc",
                "caseInsensitive": true
              },
              "newlines-between": "always"
            }
          ],
          "import/newline-after-import": [
            "warn",
            {
              "count": 1
            }
          ],
          "prefer-arrow-callback": 0,
          "prefer-template": "error",
          "no-constant-condition": [
            "error"
          ]
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "prettier": "2.6.2"
  }
}
